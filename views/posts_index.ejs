<!DOCTYPE html>
<html>
    <title></title>
    <body>
        <h1>POSTS</h1>
        <table>
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Title</th>
                    <th>User</th>
                </tr>
            </thead>
            <tbody>
                <% posts.forEach((post)=>{ %>
                    <tr>
                        <td><%= post.id %></td>
                        <td><a href="/posts/<%=post.id %>"><%= post.title %></a></td>
                        <td><%= post.user_id %></td>
                    </tr>
                <% }) %>
                <tr>

                </tr>
            </tbody>
        </table>
    </body>
</html>